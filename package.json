{
  "name": "parcel-react-template",
  "version": "0.0.1",
  "description": "Parcel React Template",
  "scripts": {
    "merge": "react3l translate merge -i src/ -o src/locales/ -p src/locales/",
    "extract": "react3l translate extract -i src/ -o src/locales/ -p src/locales/",
    "watch-chrome": "rm -rf dist .parcel-cache && NODE_ENV=development parcel watch manifest.json",
    "build-chrome": "rm -rf dist .parcel-cache && NODE_ENV=production  parcel build manifest.json --no-source-maps",
    "watch": "rm -rf dist .parcel-cache && cross-env NODE_ENV=development parcel src/index.html",
    "build": "rm -rf dist .parcel-cache && cross-env NODE_ENV=production  parcel build src/index.html --no-source-maps",
    "zip": "zip -r extension.zip dist",
    "release": "run-s build zip"
  },
  "alias": {
    "manifest.json": "./manifest.json",
    "package.json": "./package.json",
    "readme.mdx": "./README.md",
    "src": "./src"
  },
  "repository": "https://github.com/thanhtunguet/parcel-react-template",
  "author": {
    "name": "thanhtunguet",
    "email": "thanhtung.uet@gmail.com",
    "url": "https://github.com/thanhtunguet"
  },
  "maintainers": [
    {
      "name": "thanhtunguet",
      "email": "thanhtung.uet@gmail.com",
      "url": "https://github.com/thanhtunguet"
    }
  ],
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@mdx-js/react": "^1",
    "@redux-devtools/extension": "^3.2.5",
    "@reduxjs/toolkit": "^2.0.1",
    "@sentry/react": "^7.92.0",
    "@swc/helpers": "^0.4.12",
    "antd": "^5.12.7",
    "axios": "~0.21",
    "bootstrap": "^4",
    "classnames": "^2.5.0",
    "dayjs": "^1.11.10",
    "firebase": "^9.20.0",
    "history": "^5.3.0",
    "i18next": "^23.7.11",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-markdown": "^9.0.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.0",
    "react3l": "^7.0.0-rc4",
    "reactstrap": "^9.2.0",
